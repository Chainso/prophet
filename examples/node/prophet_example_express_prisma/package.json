{
  "name": "prophet_example_express_prisma",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "start": "node dist/src/server.js",
    "dev": "tsx src/server.ts",
    "test:integration": "DATABASE_URL=file:./dev.db mocha --timeout 30000 test/http-flow.spec.mjs",
    "prisma:generate": "prisma generate --schema gen/node-express/prisma/schema.prisma",
    "prisma:push": "DATABASE_URL=file:./dev.db prisma db push --schema gen/node-express/prisma/schema.prisma",
    "prophet:gen": "prophet gen",
    "prophet:check": "prophet check --show-reasons",
    "prophet:validate": "prophet validate"
  },
  "dependencies": {
    "@prophet/events-runtime": "file:../../../prophet-lib/javascript",
    "@prisma/client": "^5.22.0",
    "express": "^4.19.2",
    "prisma": "^5.22.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "mocha": "^10.8.2",
    "supertest": "^7.1.3",
    "tsx": "^4.19.1",
    "typescript": "^5.6.3"
  }
}
